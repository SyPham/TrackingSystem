@model DemoDoan.Models.Status

@{
    ViewBag.Title = "Edit";
    var langs = (IEnumerable<DemoDoan.Models.Language>)ViewBag.Language;

}



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <div class="row">
        <div class="col-lg-12">
            <h2>@StaticResource.Resource.Record_Edit</h2>
            <h4>Status</h4>
        </div>
        <div class="col-lg-12">
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            @Html.HiddenFor(model => model.StatusID)
        </div>
        <div class="col-lg-12">
            <div class="form-group">
                <label>Name VI</label>
                <input type="text" name="nameVI" class="form-control" value="@ViewBag.VI" id="nameVI" />
            </div>
        </div>
        <div class="col-lg-12">
            <div class="form-group">
                <label>Name EN</label>
                <input type="text" name="nameEN" class="form-control" value="@ViewBag.EN" id="nameEN" />
            </div>
        </div>
        <div class="col-lg-12">
            <div class="form-group">
                <label>Name TW</label>
                <input type="text" name="nameTW" class="form-control" value="@ViewBag.TW" id="nameTW" />
            </div>
        </div>
        <div class="col-lg-12">
            <div class="form-group">
                <input type="submit" StaticResource.Resource.General_Save class="btn btn-default" />
            </div>
        </div>
        <div class="col-lg-12">
            @Html.ActionLink(StaticResource.Resource.General_BackToList, "Index", null, htmlAttributes: new { @class = "btn btn-primary btn-sm" })

        </div>
    </div>

}


